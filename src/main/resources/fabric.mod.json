{
	"schemaVersion": 1,
	"id": "wikiwindowopener",
	"version": "${version}",
	"name": "Wiki Window Opener",
	"description": "An item that  opens the corresponding minecraft Wiki Page",
	"authors": [
		"Denise Mueller"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/denisemueller02/java-wiki-window"
	},
	"license": "MIT",
	"icon": "assets/wikiwindowopener/icon.png",
	"environment": "*",
