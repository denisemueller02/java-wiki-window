			"com.dmu.wikiwindowopener.WikiWindowOpener"
		],
		"fabric-datagen": [
			"com.dmu.wikiwindowopener.WikiWindowOpenerDataGenerator"
		]
	},
	"mixins": [
		"wikiwindowopener.mixins.json"
	],
	"accessWidener": "wikiwindowopener.accesswidener",
	"client":[
		"com.dmu.wikiwindowopener.WikiWindowOpenerClient"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
